&lt;h2 id=&quot;componentes&quot;&gt;Componentes&lt;/h2&gt;
&lt;h3 id=&quot;input&quot;&gt;Input&lt;/h3&gt;
&lt;ul&gt;
  &lt;li&gt;Query Box
    &lt;ul&gt;
      &lt;li&gt;Sugerencias&lt;/li&gt;
      &lt;li&gt;Comandos: site:&lt;/li&gt;
      &lt;li&gt;Comunes: significado de, traducir, etc.&lt;/li&gt;
    &lt;/ul&gt;
  &lt;/li&gt;
  &lt;li&gt;Filtros
    &lt;ul&gt;
      &lt;li&gt;Contexto (Web/Todo, Videos, Imágenes, Noticias, Mapas, Respuestas, Libros, Aplicaciones)&lt;/li&gt;
      &lt;li&gt;Fechas&lt;/li&gt;
      &lt;li&gt;Regiones / Local&lt;/li&gt;
      &lt;li&gt;Safe Search / Adult Content&lt;/li&gt;
      &lt;li&gt;Para imágenes: tipo (cara, foto, animación, clip art), resolución, layout, colores, licencia.&lt;/li&gt;
      &lt;li&gt;Para videos: resolución, duración, calidad.&lt;/li&gt;
      &lt;li&gt;Para noticias: relevancia, reciente.&lt;/li&gt;
    &lt;/ul&gt;
  &lt;/li&gt;
&lt;/ul&gt;

&lt;h3 id=&quot;output&quot;&gt;Output&lt;/h3&gt;
&lt;ul&gt;
  &lt;li&gt;Total de resultados y tiempo&lt;/li&gt;
  &lt;li&gt;Quizas quiso decir?
    &lt;ul&gt;
      &lt;li&gt;Typos&lt;/li&gt;
      &lt;li&gt;Lenguaje&lt;/li&gt;
    &lt;/ul&gt;
  &lt;/li&gt;
  &lt;li&gt;Resultados relacionados
    &lt;ul&gt;
      &lt;li&gt;&lt;strong&gt;Knowledge&lt;/strong&gt; Panel / Card&lt;/li&gt;
    &lt;/ul&gt;
  &lt;/li&gt;
  &lt;li&gt;Resultados, dependiendo del medio de búsqueda
    &lt;ul&gt;
      &lt;li&gt;Voz
        &lt;ul&gt;
          &lt;li&gt;&lt;strong&gt;Respuesta&lt;/strong&gt;, generalmente 1 respuesta&lt;/li&gt;
          &lt;li&gt;Acciones para esa respuesta&lt;/li&gt;
        &lt;/ul&gt;
      &lt;/li&gt;
      &lt;li&gt;Texto
        &lt;ul&gt;
          &lt;li&gt;&lt;strong&gt;Resultados&lt;/strong&gt;, 10 en adelante&lt;/li&gt;
          &lt;li&gt;Información: Título, fecha, link, snippet, imagen, ícono&lt;/li&gt;
          &lt;li&gt;Acciones: Similares, traducir, cache&lt;/li&gt;
        &lt;/ul&gt;
      &lt;/li&gt;
    &lt;/ul&gt;
  &lt;/li&gt;
  &lt;li&gt;Paginación&lt;/li&gt;
  &lt;li&gt;Anuncios&lt;/li&gt;
&lt;/ul&gt;

&lt;h3 id=&quot;datos&quot;&gt;Datos&lt;/h3&gt;
&lt;ul&gt;
  &lt;li&gt;Universo de páginas
    &lt;ul&gt;
      &lt;li&gt;Interconexión con otros servicios&lt;/li&gt;
      &lt;li&gt;Resultados en tiempo real&lt;/li&gt;
    &lt;/ul&gt;
  &lt;/li&gt;
  &lt;li&gt;Contexto
    &lt;ul&gt;
      &lt;li&gt;Textual / Literal&lt;/li&gt;
      &lt;li&gt;Historial&lt;/li&gt;
      &lt;li&gt;Estadísticas&lt;/li&gt;
      &lt;li&gt;MRU, MR searched&lt;/li&gt;
    &lt;/ul&gt;
  &lt;/li&gt;
  &lt;li&gt;Orden de aparición
    &lt;ul&gt;
      &lt;li&gt;Importancia&lt;/li&gt;
    &lt;/ul&gt;
  &lt;/li&gt;
&lt;/ul&gt;

